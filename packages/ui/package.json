{
  "name": "@frontegg/vue-ui",
  "version": "0.1.0",
  "description": "",
  "main": "dist/vue-ui.ssr.js",
  "browser": "dist/vue-ui.esm.js",
  "module": "dist/vue-ui.esm.js",
  "unpkg": "dist/vue-ui.min.js",
  "types": "dist/index.esm.d.ts",
  "files": [
    "dist/*",
    "vue-ui.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build:watch": "cross-env NODE_ENV=production rollup -w --config build/rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@frontegg/vue-core": "^0.1.0",
    "vue-i18n": "^8.17.3",
    "vuetify": "^2.4.0",
    "classnames": "^2.2.6",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-json": "^4.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "~2.1.0",
    "vuetify-loader": "^1.7.0",
    "typescript": "^3.8.3",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "sass": "^1.32.7",
    "sass-loader": "^10.1.1",
    "@types/classnames": "^2.2.11",
    "@types/yup": "^0.29.11"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
